<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.learning.trace.android_learning_trace">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.BasicUIComponentP1Activity" />
        <activity android:name=".view.BasicUIComponentP2Activity" />
        <activity android:name=".view.BasicUIComponentP3Activity" />
        <activity android:name=".view.BasicUIComponentP4Activity" />
        <activity android:name=".view.Part5Activity" />
        <activity android:name=".view.Part6Activity" />
        <activity android:name=".view.Part7Activity" />
        <activity android:name=".view.Part8Activity" />
        <activity android:name=".view.Part9Activity" />
    </application>

</manifest>